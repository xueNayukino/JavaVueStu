<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.cuit.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="cn.cuit.domain.Student">
            <id property="studentId" column="student_id" />
            <result property="photo" column="photo" />
            <result property="onCampus" column="on_campus" />
            <result property="studentNo" column="student_no" />
            <result property="registrationStatus" column="registration_status" />
            <result property="phone" column="phone" />
            <result property="idCard" column="id_card" />
            <result property="gender" column="gender" />
            <result property="name" column="name" />
            <result property="birthDate" column="birth_date" />
    </resultMap>

    <sql id="Base_Column_List">
        student_id,photo,on_campus,student_no,registration_status,phone,
        id_card,gender,name,birth_date
    </sql>
</mapper>
